<table border = "1">
  <thead>
    <th>person_id</th>
    <th>Name</th>
    <th>dob</th>
    <th>Gender</th>
  </thead>

  <tbody>
    <% Person.all.each do |person| %>
    <tr>
      <td><%= person.id %></td>
      <td><%= person.name %></td>
      <td><%= person.dob %></td>
      <td><%= person.gender %></td>
    </tr>
    <% end %>
  </tbody>
</table>